{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Launch FastAPI (uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "src.app:app",
        "--reload",
        "--port",
        "8000"
      ],
      "jinja": true,
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}